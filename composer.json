{
    "name": "detosphere-ltd/laravel-faqs",
    "description": "Frequently asked questions crud implementation",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "faqs",
        "frequently-asked-questions"
    ],
    "autoload": {
        "psr-4": {
            "DetosphereLtd\\LaravelFaqs\\": "src/",
            "DetosphereLtd\\LaravelFaqs\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DetosphereLtd\\LaravelFaqs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "DetosphereLtd\\LaravelFaqs\\FAQServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "alhaji-aki/laravel-uuid": "^1.0",
        "illuminate/database": "^7.30|^8.0",
        "illuminate/support": "^7.30|^8.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^5.2|^6.11",
        "phpunit/phpunit": "^9.5.1",
        "spatie/macroable": "^1.0"
    },
    "scripts": {
        "test": "phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
